function tab_li2 () {
	$( '.tab_type1' ).each( function () {
		var $this = $( this );
		var $li = $this.find( 'li' );
		var $liLeg = $li.length;
		if ( $liLeg ) $this.addClass( 'li' + $liLeg );
	} );
};
