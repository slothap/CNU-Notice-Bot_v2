			spaceBetween : 20 , // 슬라이드 간격
			navigation : {
				nextEl : ".prg_swiper_wrap .swiper-button-next" ,
				prevEl : ".prg_swiper_wrap .swiper-button-prev" ,
			} ,
			pagination : {
				el : ".prg_swiper_wrap .swiper-pagination" ,
				clickable : true , // 클릭 가능
			} ,
			breakpoints : {
				1280 : {
					slidesPerView : 2 ,
				} ,
				760 : {
					slidesPerView : 1 ,
				} ,
			}
