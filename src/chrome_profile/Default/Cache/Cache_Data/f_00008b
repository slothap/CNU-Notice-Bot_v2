			pagination : {
				el : ".program_wrap .swiper-pagination" ,
				clickable : true , // 클릭 가능
			} ,
			breakpoints : {
				1024 : {
					slidesPerView : 2 ,
				} ,
				760 : {
					slidesPerView : 1 ,
				} ,
			}
		/* loop : true , // 무한 반복 */
		} );
		
		// 비교과 프로그램 슬라이드 (로그인 후)
